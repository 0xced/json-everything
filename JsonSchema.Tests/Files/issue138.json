{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "Receiver schema for pia-shopdb-pub",
  "type": "object",
  "required": [
    "eventType",
    "timestamp",
    "eventId",
    "publisherId",
    "NotifyPartnerDetails"
  ],
  "properties": {
    "eventId": {
      "type": "string"
    },
    "publisherId": {
      "type": "string"
    },
    "NotifyPartnerDetails": {
      "minItems": 0,
      "type": "object",
      "properties": {
        "AdditionalInformation": {
          "type": "object",
          "properties": {
            "Language": {
              "type": "string"
            },
            "Remark": {
              "type": "string"
            }
          }
        },
        "Skills": {
          "type": "object",
          "properties": {
            "SelfinstalledAllowed": {
              "description": "0 - false, 1 - true",
              "type": "string"
            },
            "BasedRetailerInstallation": {
              "type": "string"
            }
          }
        },
        "POBoxAddress": {
          "type": "object",
          "properties": {
            "uniqueAddressIdGAIA": {
              "type": "string"
            },
            "POBox": {
              "type": "string"
            },
            "Country": {
              "type": "string"
            },
            "positionGAIA": {
              "type": "string"
            },
            "City": {
              "type": "string"
            },
            "PLZ": {
              "type": "string"
            }
          }
        },
        "Channel": {
          "type": "object",
          "properties": {
            "Subkanal": {
              "type": "string"
            },
            "SubkanalId": {
              "type": "string"
            },
            "OwnThird": {
              "type": "string"
            },
            "HauptkanalId": {
              "type": "string"
            },
            "OMSRating": {
              "type": "string"
            },
            "SMERegion": {
              "type": "string"
            },
            "SMESubregion": {
              "type": "string"
            },
            "OptPBX": {
              "type": "string"
            },
            "RatingCategoryId": {
              "type": "string"
            },
            "RatingTypeId": {
              "type": "string"
            },
            "Hauptkanal": {
              "type": "string"
            },
            "LevelUp": {
              "type": "string"
            }
          }
        },
        "LeadManagementInformation": {
          "type": "object",
          "properties": {
            "LeadManagementAllowed": {
              "description": "0 - false, 1 - true",
              "type": "string"
            },
            "LeadManagementEmail": {
              "type": "string"
            }
          }
        },
        "LocationAddress": {
          "type": "object",
          "properties": {
            "uniqueAddressIdGAIA": {
              "type": "string"
            },
            "HouseNumber": {
              "type": "string"
            },
            "Coordinates": {
              "type": "object",
              "properties": {
                "Latitude": {
                  "type": "string"
                },
                "Longitude": {
                  "type": "string"
                }
              }
            },
            "Street": {
              "type": "string"
            },
            "positionGAIA": {
              "type": "string"
            },
            "City": {
              "type": "string"
            },
            "PLZ": {
              "type": "string"
            }
          }
        },
        "Routing": {
          "type": "integer"
        },
        "AccountManager": {
          "type": "object",
          "properties": {
            "Active": {
              "description": "0 - false, 1 - true",
              "type": "string"
            },
            "Nachname": {
              "type": "string"
            },
            "Mobilephone": {
              "type": "string"
            },
            "Geschlecht": {
              "type": "string"
            },
            "Sprache": {
              "type": "string"
            },
            "Vorname": {
              "type": "string"
            }
          }
        },
        "Identifiers": {
          "type": "object",
          "properties": {
            "Partner_ID": {
              "type": "string"
            },
            "EEV_Nbr": {
              "type": "string"
            },
            "BrokerID": {
              "type": "string"
            },
            "VAT_Nbr": {
              "type": "string"
            },
            "SalesOffice": {
              "type": "string"
            },
            "PointOfSaleID": {
              "type": "string"
            },
            "HoldingNbr": {
              "type": "string"
            },
            "DealerId": {
              "type": "string"
            },
            "KreditorNumber": {
              "type": "string"
            },
            "SCN_ENT": {
              "type": "string"
            },
            "Account_payable_nbr": {
              "type": "integer"
            },
            "BankType": {
              "type": "string"
            },
            "Internal_ID": {
              "type": "integer"
            },
            "Mandant": {
              "type": "string"
            },
            "SCN": {
              "type": "string"
            }
          }
        },
        "ValidityInformation": {
          "type": "object",
          "properties": {
            "ValidTo": {
              "format": "date",
              "description": "Format yyyy-MM-ddThh:mm:ss",
              "type": "string"
            },
            "active": {
              "description": "0 - false, 1 - true",
              "type": "string"
            },
            "ValidFrom": {
              "format": "date",
              "description": "Format yyyy-MM-ddThh:mm:ss",
              "type": "string"
            },
            "AddressChangeDate": {
              "format": "date",
              "description": "Format yyyy-MM-dd",
              "type": "string"
            }
          }
        },
        "Contact": {
          "type": "object",
          "properties": {
            "Web": {
              "type": "string"
            },
            "Phone": {
              "type": "string"
            },
            "Fax": {
              "type": "string"
            },
            "eMail": {
              "type": "string"
            }
          }
        },
        "Name": {
          "type": "object",
          "properties": {
            "Name1": {
              "type": "string"
            },
            "Name2": {
              "type": "string"
            }
          }
        }
      }
    },
    "eventType": {
      "type": "string"
    },
    "timestamp": {
      "format": "date-time",
      "type": "string"
    }
  }
}