{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Json.More.csproj"
          ],
          "src":"../Json.More/"
        },
        {
          "files": [
            "JsonPointer.csproj"
          ],
          "src":"../JsonPointer/"
        },
        {
          "files": [
            "JsonSchema.csproj"
          ],
          "src":"../JsonSchema/"
        },
        {
          "files": [
            "JsonSchema.Generation.csproj"
          ],
          "src":"../JsonSchema.Generation/"
        },
        {
          "files": [
            "JsonPath.csproj"
          ],
          "src":"../JsonPath/"
        },
        {
          "files": [
            "JsonPatch.csproj"
          ],
          "src":"../JsonPatch/"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "usage/**.md",
          "usage/**/toc.yml",
          "examples/**.md",
          "examples/**/toc.yml",
          "release-notes/**.md",
          "release-notes/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/darkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}