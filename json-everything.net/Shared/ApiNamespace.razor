@using Radzen.Blazor

<RadzenPanelMenuItem Text="@Namespace.Name" Icon="menu_book" Expanded="false" class="ps-3 radzen-menulistitem">
    @foreach (var ns in Namespace.Namespaces)
    {
	    <ApiNamespace Namespace="@ns"/>
    }
    @foreach (var type in Namespace.Types)
    {
	    <RadzenPanelMenuItem Text="@type.Name" Value="@($"type/{type.FullName}")"/>
    }
</RadzenPanelMenuItem>

@code {

	

#pragma warning disable CS8618
    [Parameter]
    public NamespaceNode Namespace { get; set; }
#pragma warning restore CS8618
}
