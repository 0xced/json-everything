@using Radzen.Blazor
@using JsonEverythingNet.Services

@foreach (var ns in Namespace.Namespaces)
{
    <RadzenPanelMenuItem Text="@ns.Name" Icon="data_object" Expanded="false" class="ps-3 radzen-menulistitem">
	    <ApiNamespace Namespace="@ns"/>
	</RadzenPanelMenuItem>
}
@foreach (var type in Namespace.Types)
{
    <RadzenPanelMenuItem Text="@type.CSharpName()" Value="@($"type/{type.FullName}")" class="ps-3 radzen-menulistitem"/>
}

@code {
#pragma warning disable CS8618
    [Parameter]
    public NamespaceNode Namespace { get; set; }
#pragma warning restore CS8618
}
